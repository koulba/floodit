{
  "name": "floodit-frontend",
  "version": "0.0.0",
  "description": "Flood It frontend",
  "license": "MIT",
  "author": "Simon Oulevay <npm@alphahydrae.com>",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=8048 ng build",
    "check": "npm run format",
    "check:write": "npm run format:write",
    "format": "prettier --config ../.prettierrc.json --check .",
    "format:write": "prettier --config ../.prettierrc.json --write .",
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=8048 ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "^18.2.11",
    "@angular/common": "^18.2.11",
    "@angular/compiler": "^18.2.11",
    "@angular/core": "^18.2.11",
    "@angular/forms": "^18.2.11",
    "@angular/platform-browser": "^18.2.11",
    "@angular/platform-browser-dynamic": "^18.2.11",
    "@angular/router": "^18.2.11",
    "@types/luxon": "^3.0.0",
    "daisyui": "^4.12.14",
    "lodash-es": "^4.17.21",
    "luxon": "^3.0.1",
    "ngx-toastr": "^19.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular/build": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.11",
    "@types/lodash-es": "^4.17.6",
    "autoprefixer": "^10.4.8",
    "cross-env": "^10.1.0",
    "postcss": "^8.4.16",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.1.8",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 22",
    "npm": ">= 7"
  }
}
